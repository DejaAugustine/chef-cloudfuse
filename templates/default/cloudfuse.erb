#
# ~/.cloudfuse
#
# Generated by Chef for <%= node[:fqdn] %>
#

username=<%= @username %>
api_key=<%= @api_key %>
authurl=<%= @authurl %>
<% unless @use_snet.nil? %>
use_snet=<%= @use_snet %> #[True to use snet for connections]
<% end %>
<% unless @cache_timeout.nil? %>
cache_timeout=<%= @cache_timeout %> #[seconds for directory caching, default 600]
<% end %>